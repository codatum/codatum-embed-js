{
  "name": "@codatum/embed-react",
  "version": "0.4.0",
  "description": "React integration for Codatum signed embedding",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "biome lint .",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@codatum/embed": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "catalog:",
    "@testing-library/react": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.12"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/codatum/codatum-embed-js.git",
    "directory": "packages/embed-react"
  },
  "homepage": "https://github.com/codatum/codatum-embed-js/tree/main/packages/embed-react#readme",
  "bugs": {
    "url": "https://github.com/codatum/codatum-embed-js/issues"
  },
  "keywords": [
    "codatum",
    "embed",
    "react",
    "signed-embed",
    "analytics",
    "notebook"
  ],
  "publishConfig": {
    "access": "public"
  }
}
