{
  "name": "@codatum/embed",
  "version": "0.2.0",
  "description": "TypeScript library for Codatum signed embedding",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && tsup --config tsup.global.config.ts",
    "dev": "tsup --watch",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "biome lint .",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "catalog:",
    "jsdom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.12"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/codatum/codatum-embed-js.git",
    "directory": "packages/embed"
  },
  "homepage": "https://github.com/codatum/codatum-embed-js/tree/main/packages/embed#readme",
  "bugs": {
    "url": "https://github.com/codatum/codatum-embed-js/issues"
  },
  "keywords": [
    "codatum",
    "embed",
    "iframe",
    "signed-embed",
    "analytics",
    "notebook"
  ],
  "publishConfig": {
    "access": "public"
  }
}
