{
  "name": "@codatum/embed",
  "version": "0.0.0",
  "description": "TypeScript library for Codatum signed embedding",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint .",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "jsdom": "^25.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
